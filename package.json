{
  "name": "monad_presentation",
  "version": "0.0.1",
  "description": "Monad presentation for Wellington JS meetup",
  "author": "Paul Johnson <paul@johnson.kiwi.nz>",
  "scripts": {
    "server": "http-server .",
    "styles": "sass monads.scss",
    "preprocess": "./preprocessor.js monads.js > monads.md",
    "develop":
         "livereload & sass --watch monads.scss & watch \"npm run-script preprocess\"",
    "test": "mocha --harmony monads.js",
    "test:watch": "mocha --watch --harmony monads.js"
  },
  "main": "monads.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pj/monad_presentation"
  },
  "bugs": {
    "url": "https://github.com/pj/monad_presentation/issues"
  },
  "keywords": [
    "monads",
    "wellingtonjs",
    "presentation"
  ],
  "analyze": true,
  "license": "MIT",
  "dependencies": {
    "commander": "^2.8.1",
    "esprima": "^2.6.0",
    "http-server": "^0.8.5",
    "watch": "^0.16.0"
  }
}
